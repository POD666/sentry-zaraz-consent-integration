{
  "name": "sentry-zaraz-consent-integration",
  "version": "3.0.0",
  "type": "module",
  "description": "A wrapper around @imviidx/sentry-consent-integration specifically for Cloudflare Zaraz consent management",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "demo:dev": "cd demo && npm run dev",
    "demo:install": "cd demo && npm install",
    "demo:build": "cd demo && npm run build",
    "test": "echo \"No tests yet\" && exit 0",
    "release": "np",
    "release:dry": "np --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/POD666/sentry-zaraz-consent-integration.git"
  },
  "keywords": [
    "sentry",
    "consent",
    "gdpr",
    "privacy",
    "zaraz",
    "cloudflare"
  ],
  "author": "v.v.danyliuk@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/POD666/sentry-zaraz-consent-integration/issues"
  },
  "homepage": "https://github.com/POD666/sentry-zaraz-consent-integration#readme",
  "dependencies": {
    "@imviidx/sentry-consent-integration": "^0.1.2",
    "zaraz-ts": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2",
    "np": "^10.2.0"
  },
  "peerDependencies": {
    "@sentry/browser": "^8.29.0",
    "@sentry/react": "^8.29.0",
    "@sentry/vue": "^8.29.0",
    "@sentry/types": "^8.29.0"
  },
  "peerDependenciesMeta": {
    "@sentry/browser": {
      "optional": true
    },
    "@sentry/react": {
      "optional": true
    },
    "@sentry/vue": {
      "optional": true
    }
  }
}
